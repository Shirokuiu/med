                      +svg('calendar1')
                    p.fillial__slide-list-item-text
                      span.fillial__slide-list-item-text-item Пн-Сб: 9:00 - 20:00,
                      span.fillial__slide-list-item-text-item Вс: 10:00 - 19:00
                  li.fillial__slide-list-items.fillial__slide-list-items--flex-wrap
                    span.fillial__slide-list-item-icon.fillial__slide-list-item-icon--metro-mod4
                      +svg('moscow1')
                    p.fillial__slide-list-item-text Каховская/Севастопольская
                    .fillial__slide-list-item-note-wrap
                      span.fillial__slide-list-item-note-icon
                        +svg('walk1')
                      span.fillial__slide-list-item-note-text 200 м
